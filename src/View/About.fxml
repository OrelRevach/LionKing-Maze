<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/9.0.1" fx:controller="View.AboutController">
    <BorderPane>
        <bottom>
            <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Close"
                    BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets bottom="10.0"/>
                </BorderPane.margin>
            </Button>
        </bottom>
        <center>
            <ScrollPane prefHeight="365.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                <Label fx:id="Itext" text="About">
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                </Label>
            </ScrollPane>
        </center>
    </BorderPane>
</AnchorPane>
